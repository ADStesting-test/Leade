npc2_destination_state = (npc2_destination); 
npc2=Vehicle(npc2_init_state, npc2_waypoints, npc2_destination, npc2_type);

ped1_type = "Adult";
ped1_heading = 0 deg related to ego_vehicle;
ped1_init_position = ("lane_2"->15); 
ped1_init_state = (ped1_init_position, ped1_heading,2); 
ped1_waypoints = Waypoint(("lane_2"->15,0,2),("lane_7"->35,30,3),("lane_14"->70,0,2)); 
