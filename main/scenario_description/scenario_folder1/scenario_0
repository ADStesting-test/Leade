ego_vehicle = AV(ego_init_state, ego_target_state,vehicle_type);

npc1_type = "Truck";
npc1_heading = 10 deg related to ego_vehicle;
npc1_init_position = ("lane_2"->10); 
npc1_init_state = (npc1_init_position, npc1_heading,15); 
npc1_waypoints = Waypoint(("lane_2"->10,10,15),("lane_4"->35,16,12),("lane_14"->70,25,10)); 
npc1_destination = ("lane_14"->70); 
npc1_destination_state = (npc1_destination); 
npc1=Vehicle(npc1_init_state, npc1_waypoints, npc1_destination, npc1_type); 

npc2_type = "Bicycle";
npc2_heading = 0 deg related to ego_vehicle;
